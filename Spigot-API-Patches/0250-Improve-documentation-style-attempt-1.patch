Subject: [PATCH] Improve documentation style attempt 1.


diff --git a/javadoc-style.css b/javadoc-style.css
index 98055b22d6d5bed7792d6857aa760cf914c62653..c74cdde5e7bd1405a36922ba5d36214b89c1484f 100644
--- a/javadoc-style.css
+++ b/javadoc-style.css
@@ -18,7 +18,7 @@ a:link, a:visited {
 }
 a:hover, a:focus {
     text-decoration:none;
-    color:#bb7a2a;
+    color: #2be28c;
 }
 a:active {
     text-decoration:none;
@@ -101,7 +101,7 @@ Document title and Copyright styles
     text-decoration:none;
 }
 .bar a:hover, .bar a:focus {
-    color:#bb7a2a;
+    color:#2be28c;
 }
 .tab {
     background-color:#0066FF;
@@ -122,7 +122,7 @@ Navigation bar styles
     margin:0;
 }
 .topNav {
-    background-color:#4D7A97;
+    background-color:#00a59d;
     color:#FFFFFF;
     float:left;
     padding:0;
@@ -181,11 +181,11 @@ ul.subNavList li{
 }
 .topNav a:hover, .bottomNav a:hover {
     text-decoration:none;
-    color:#bb7a2a;
+    color:#1df8ee;
     text-transform:uppercase;
 }
 .navBarCell1Rev {
-    background-color:#F8981D;
+    background-color:#1de4f8;
     color:#253441;
     margin: auto 5px;
 }
@@ -400,7 +400,7 @@ Table styles
     padding-bottom:7px;
     display:inline-block;
     float:left;
-    background-color:#F8981D;
+    background-color:#1df8ee;
     border: none;
     height:16px;
 }
@@ -412,7 +412,7 @@ Table styles
     margin-right:3px;
     display:inline-block;
     float:left;
-    background-color:#F8981D;
+    background-color:#1df8ee;
     height:16px;
 }
 .memberSummary caption span.tableTab span {
@@ -440,7 +440,7 @@ Table styles
     width:5px;
     position:relative;
     float:left;
-    background-color:#F8981D;
+    background-color:#1df8ee;
 }
 .memberSummary .activeTableTab .tabEnd {
     display:none;
@@ -448,7 +448,7 @@ Table styles
     margin-right:3px;
     position:relative; 
     float:left;
-    background-color:#F8981D;
+    background-color:#1df8ee;
 }
 .memberSummary .tableTab .tabEnd {
     display:none;
